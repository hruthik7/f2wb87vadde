extends layout

block content
  h1= title
    -let image_names = ["rabbit1_300x300.jpg","rabbit2_300x300.jpg","rabbit3_300x300.jpg","rabbit4_300x300.jpg","rabbit5_1_300x300.jpg"];
  - for(let i=0;i<3;i++){
      - let h = Math.floor(Math.random()*5);
      - let pic = image_names[h];
      img(src=`/images/${pic}` alt=`${pic}` style="height:300px; width:300px")
  - } 



